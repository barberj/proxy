div class="api"
  div class="col-xs-12 col-sm-3 col-md-3 col-lg-3"
    div class="api-box"
      span
        | {{{ image }}}
      button[
        class="btn btn-primary btn-xs api edit-api"
        data-id="{{id}}"
      ] Edit
      button[
        class="btn btn-danger btn-xs api del-api"
        data-id="{{id}}"
      ] Delete
